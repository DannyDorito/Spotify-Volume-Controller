<Window x:Class="SpotifyVolumeController.UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SpotifyVolumeController.UI"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Unloaded="Window_Unloaded">
    <Grid>
        <ListBox x:Name="DebugBox" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,57,0,-57" IsEnabled="False"/>
        <Button x:Name="Auth" Content="Button" HorizontalAlignment="Left" Margin="437,34,0,0" VerticalAlignment="Top" Click="Auth_Click"/>

    </Grid>
</Window>
